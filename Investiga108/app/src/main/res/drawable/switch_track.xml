<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Estado cuando el switch est치 activado (ON) -->
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#4CAF50" /> <!-- Color de fondo del track cuando est치 ON -->
        </shape>
    </item>
    <!-- Estado cuando el switch est치 desactivado (OFF) -->
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#BDBDBD" /> <!-- Color de fondo del track cuando est치 OFF -->
        </shape>
    </item>
</selector>
